{
"name": "Fantabulous",
"version": "0.0.1",
"manifest_version": 2,
"description": "Improved New Tab Page",

"icons": {
"16": "icons/icon16.png",
"48": "icons/icon48.png",
"128": "icons/icon128.png"
},

"default_locale": "en",

"chrome_url_overrides": {
"newtab": "src/override/override.html"
},

"permissions": [
  "https://source.unsplash.com/",
  "http://quotes.rest/",
  "https://api.openweathermap.org/data/2.5/weather?",
  "geolocation"
]
}
